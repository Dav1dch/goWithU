.avatar {
    position: relative;
    z-index: 1;
    overflow: hidden;
    border-radius: 50%;
    height: 170rpx;
    width: 170rpx;
    border: 2px solid #fff;
    box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);

}

.vitem {
    padding-top: 150rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;

}